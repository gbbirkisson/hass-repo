{
    "image": "gbbirkisson/xcomfortd",
    "name": "XComfortd",
    "version": "0.1",
    "slug": "xcomfortd",
    "description": "Eaton XComfort to MQTT gateway",
    "startup": "application",
    "boot": "auto",
    "arch": [
        "aarch64",
        "amd64",
        "armhf",
        "armv7",
        "i386"
    ],
    "privileged": [
        "SYS_RAWIO"
    ],
    "devices": [
        "/dev/bus/usb:/dev/bus/usb:rwm"
    ],
    "map": [
        "config"
    ],
    "options": {
        "mqtt_host": "localhost",
        "mqtt_port": "1883",
        "mqtt_user": "username",
        "mqtt_pass": "password",
        "verbose": false
    },
    "schema": {
        "mqtt_host": "str",
        "mqtt_port": "int",
        "mqtt_user": "str",
        "mqtt_pass": "str",
        "verbose": "bool"
    }
}